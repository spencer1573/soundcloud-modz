{
  "name": "SoundCloud Mods",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Mods to Sound Cloud",
  "homepage_url": "https://github.com/spencer1573",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "** soundcloud mods **"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ]
}